---
import AILayout from "../layouts/AILayout.astro";
import IAHeader from "../components/IA/Header/IAHeader.astro";
import IAMain from "../components/IA/Main/IAMain.astro";
---

<AILayout title="PymeWebTeam">
    <IAHeader></IAHeader>
    <IAMain></IAMain>
</AILayout>

<style is:global>
    *{
        font-family: 'Poppins', sans-serif;
    }

    body{
        background-color: #0B0E14;
        min-height: 100vh;
        width: 100vw;
        flex-direction: column;
        display: flex;
        align-items: center;
    }
</style>